var init_end : bool
var x : int
main process mainProcess {
    main procedure mainProcedure (){
        var a : int
        init loc L0
        final loc Lf
        L0 -> Lf {
            a := 0
            a -> x
            init_end := true
        }
    }
}
process p0 {
    main procedure mainProcedure (){
        var a : int
        init loc L0
        final loc Lf
        L0 -> Lf {
            assume init_end
            a := 1
            a -> x
        }
    }
}
process p1 {
    main procedure mainProcedure (){
        var a : int

        init loc L0
        final loc Lf

        L0 -> Lf {
            assume init_end
            a <- x
        }
    }
}