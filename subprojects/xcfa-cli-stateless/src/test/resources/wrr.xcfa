var x : int (0)
process p0 {
    main procedure mainProcedure (){
        var a : int
        var b : int

        init loc L0
        loc L1
        final loc Lf

        L0 -> L1 {
            a <- x
        }

        L1 -> Lf {
            b <- x
        }
    }
}
main process p1 {
    main procedure mainProcedure (){
        var a : int
        init loc L0
        loc L1
        final loc Lf
        L0 -> L1 {
            a := 1
            a -> x
        }
        L1 -> Lf {
            a := 2
            a -> x
        }
    }
}