static simple
main process p0 {
    main procedure mainProcedure (){
        var a : int
        init loc L0
        loc L1
        error loc Le
        final loc Lf
        L0 -> L1 {
            call func(a OUT, 1 IN, 2 INOUT)
        }
        L1 -> Lf {
            assume (a = 3)
        }
        L1 -> Le {
            assume not (a = 3)
        }
    }
    int procedure func(a : int, b : int) {
        var c : int
        init loc L1
        final loc L2
        L1->L2 {
            c := b
            b := 4
            return a+c
        }
    }
}